
<article class="show-result">
  <div class="bread-nav">
    <ul class="breadcrumb-list">
       <li>
          <span>
            Path
          </span>
          <svg width="22" height="22" viewBox="0 0 18 18" xmlns="http://www.w3.org/2000/svg" class="ui-icon ui-icon--chevron-right">
             <g fill="#000" fill-rule="evenodd">
                <path d="M6.646 5.354l4 4 .354.353.707-.707-.353-.354-4-4L7 4.293 6.293 5z"></path>
                <path d="M7.354 13.354l4-4L11.707 9 11 8.293l-.354.353-4 4-.353.354.707.707z"></path>
             </g>
          </svg>
       </li>
       <li>
         <span>
           for
         </span>
          <svg width="22" height="22" viewBox="0 0 18 18" xmlns="http://www.w3.org/2000/svg" class="ui-icon ui-icon--chevron-right">
             <g fill="#000" fill-rule="evenodd">
                <path d="M6.646 5.354l4 4 .354.353.707-.707-.353-.354-4-4L7 4.293 6.293 5z"></path>
                <path d="M7.354 13.354l4-4L11.707 9 11 8.293l-.354.353-4 4-.353.354.707.707z"></path>
             </g>
          </svg>
       </li>
       <li>
         <span>
           {{titlePdp}}
         </span>
       </li>
    </ul>
  </div>
  <figure class="ch-carousel">
    <div id="dm_crs_df">
      <ul>
        {{# each pictures as |pic|}}
          {{#if @first}}
            <li>
              <img id="{{pic.id}}" itemprop="image" src="{{pic.url}}">
            </li>
          {{/if}}
        {{/each}}
      </ul>
    </div>
  </figure>
  <div class="buy-box">
    <h1 class="title">
      {{titlePdp}}
    </h1>
    <div class="price">$ {{price}}</div>
    <div class="rate">
      <span class="ch-icon-star full"></span>
      <span class="ch-icon-star full"></span>
      <span class="ch-icon-star full"></span>
      <span class="ch-icon-star"></span>
      <span class="ch-icon-star"></span>
    </div>
    {{#if freeShipping}}
      <div class="free-shipping">
        <span class="ch-icon-truck"></span>
        Envío gratis
      </div>
    {{/if}}
    <input type="button" class="ch-btn" title="titlePdp" value="Comprar" />
  </div>
  <div class="description">
    <h2>Descripción del producto</h2>
    <p>{{{descriptionPlainTxt}}}</p>
    <img src="{{descriptionSnapshotUrl}}" alt="">
  </div>
</article>
